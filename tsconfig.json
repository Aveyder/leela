{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2016",
    "lib": ["es5", "dom"],
    "outDir": "dist/server",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "types"]
}
